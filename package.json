  },
  "devDependencies": {
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "prettier": {
    "rules": {
      "at-rule-empty-line-before": [
        "always",
        {
          "ignoreAtRules": [
            "else"
          ]
        }
      ],
      "block-opening-brace-space-before": "always",
      "block-closing-brace-newline-after": [
        "always",
        {
          "ignoreAtRules": [
            "if",
            "else"
          ]
        }
      ]
    }
  }
}
